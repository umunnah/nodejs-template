{
  "name": "express-template",
  "version": "1.0.0",
  "description": "express mvc template",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --exec babel-node src/server.js",
    "build": "babel src --out-dir build",
    "knex": "babel-node node_modules/.bin/knex --knexfile knexfile.js --env knexDB",
    "make:migration": "yarn knex migrate:make",
    "migrate": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "make:seed": "yarn knex seed:make",
    "db:seed": "yarn knex seed:run"
  },
  "author": "Umunnah Arinze",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "express-validator": "6.2.0",
    "faker": "4.1.0",
    "knex": "0.20.0",
    "lodash": "4.17.15",
    "method-override": "3.0.0",
    "objection": "1.6.11",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.12.1",
    "cloudinary": "^1.25.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/preset-env": "7.6.3",
    "dotenv": "8.2.0",
    "nodemon": "1.19.4",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0"
  }
}
