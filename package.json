{
  "name": "express-template",
  "version": "1.0.0",
  "description": "express mvc template",
  "main": "src/server.js",
  "engines": {
    "node": "~v16",
    "npm": "~7.10"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "SET NODE_ENV=development & babel-node src/server.js",
    "build": "babel src --out-dir build",
    "knex": "npx babel-node node_modules/knex/bin/cli.js",
    "make:migration": "npx knex migrate:make",
    "migrate": "npx knex migrate:latest",
    "migrate:rollback": "npx knex migrate:rollback",
    "make:seed": "npm knex seed:make",
    "db:seed": "npm knex seed:run"
  },
  "author": "Umunnah Arinze",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "cloudinary": "^1.25.1",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-validator": "6.2.0",
    "faker": "4.1.0",
    "helmet": "^4.5.0",
    "hpp": "^0.2.3",
    "knex": "0.95.4",
    "lodash": "4.17.15",
    "method-override": "3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "objection": "1.6.11",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.6.0",
    "uuidv4": "^6.2.7",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.13.16",
    "@babel/node": "7.13.13",
    "@babel/plugin-proposal-decorators": "7.13.15",
    "@babel/preset-env": "7.14.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "nodemon": "1.19.4",
    "sqlite3": "^5.0.2"
  }
}
